{
  "name": "DEW",
  "version": "1.1",
  "description": "Discord Export for Wordle. Appends player's guesses as spoiler-hidden text for posting in Discord.",
  "homepage_url": "https://github.com/kristaboone/discord-export-for-wordle",
  "manifest_version": 3,
  "minimum_chrome_version": "100",
  "browser_specific_settings": {
    "gecko": {
      "id": "kristacboone@gmail.com",
      "strict_min_version": "100.0"
    }
  },
  "icons": {
    "48": "icons/dew-48.png"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.nytimes.com/games/wordle/*"
      ],
      "js": [
        "browsertype.js",
        "settings.js",
        "dew.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html"
  }
}